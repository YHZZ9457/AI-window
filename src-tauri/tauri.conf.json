{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ai-window",
  "version": "0.3.0",
  "identifier": "com.a1595.ai-window",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:15173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "ai-window",
        "width": 600,
        "height": 400,
        "transparent": false,
        "decorations": true
      }
    ],
    "security": {
      "csp": null,
      "freezePrototype": true
    }
  },
  "plugins": {
    "ipc": {
      "codegen": true
    },
    "shell": {
      "open": true
    }
  }
  ,
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
